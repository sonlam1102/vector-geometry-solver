                    TestR:={op(TestR),[op(f[1]),i]};
                   if (IsVecto0(VT)=true) or VT = 0 then return Sol; fi;
             fi;           
              
   
       od;
   od;
od;</Font>
</Font>
<Font bold="false">#khi flag=false
<Font encoding="UTF-8">#VT l\303\240 k\341\272\277t qu\341\272\243 cu\341\273\221i c\303\271ng khi th\341\273\261c hi\341\273\207n ph\303\251p c\341\273\231ng vector,CongVecto(lhs(goal)-rhs(goal))[1] \303\241p d\341\273\245ng c\303\241c lu\341\272\255t c\341\273\247a \304\221\341\273\201 b\303\240i</Font>
temp:=[Sol[nops(Sol)][3]];<Font foreground="[0,0,0]">
