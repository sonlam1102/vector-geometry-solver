<Section collapsed="false" isCollapsible="true" drawButton="true" MultipleChoiceAnswerIndex="-1" MultipleChoiceRandomizeChoices="false" TrueFalseAnswerIndex="-1" EssayAnswerRows="5" EssayAnswerColumns="60"><Title><Text-field style="Heading 2" layout="Heading 2"><Font encoding="UTF-8">SolveVecto_3:=proc(goal)=&gt; T\303\255nh gi\303\241 tr\341\273\213 thu\341\273\231c t\303\255nh c\341\273\247a \304\221\341\273\221i t\306\260\341\273\243ng</Font></Text-field></Title><Text-field style="Text" layout="Normal"></Text-field>
<Group labelreference="L1033" drawlabel="true">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal"><Equation executable="true" style="2D Input" input-equation="SolveVectoLib[SolveVecto_3] := proc (goal) local VT, VP, ApplyRules, DoanVecto, listDV, kindFacts, btChen, expr, Facts3, thayVectoDoi, KiemTraChen, i; global flag, FactSet, FactSet0, FactSetOb, Sol, chiso, TestR, fSet; expr := goal; FactSet := Hypos; FactSet0 := {}; FactSetOb := {}; flag := true; Sol := []; TestR := []; DoanVecto := proc (expr) local setVar, kq, i, temp, expr1; kq := []; expr1 := thayVectoDoi(expr); expr1 := [op(expr1), op(expr)]; expr1 := subs(Doan = Vecto, expr1); for i in expr1 do if has(i, Vecto) = true then kq := [op(kq), subs([lhs(i) = lhs(i)^2, rhs(i) = rhs(i)^2], i)] end if end do; return kq end proc; thayVectoDoi := proc (expr) local set_var, sett, i; sett := []; set_var := Set_Vars(expr); for i in set_var do sett := [op(sett), i = Vectodoi(i)] end do; if 0 &lt; nops(sett) then return subs(sett, expr) end if; return expr end proc; KiemTraChen := proc (DiemChen, expr) local set_var, sett, i, temp; sett := []; set_var := Set_Vars(kq); for i in set_var do temp := ChenDiemBT(I, DiemChen); if nops(Set_Vars(temp) minus Set_Vars(expr)) = 0 then return [i^2, temp^2] end if end do; return {} end proc; ApplyRules := proc (exp) local flag2, chen, f, temp, exp1; flag2 := true; exp1 := exp; while flag2 = true do for f in fSet do for i in f[2] do if member([op(f[1]), i], TestR) then next end if; temp := MySubs(i, exp1); if Lap_LoiGiai(exp1, temp) = false then flag := true; exp1 := temp; Sol := [op(Sol), [[[op(f[1])], [i]], [convert(exp1, string)], exp1]]; TestR := {op(TestR), [op(f[1]), i]}; chen := DiemChen(exp = 0); if 0 &lt; nops(chen) then temp := KiemTraChen(exp1, chen); if 0 &lt; nops(temp) then return temp end if; chen := [] end if end if end do end do end do end proc; if 0 &lt; nops(findKinds[3]) then Facts3 := DoanVecto(factKinds[3]) else Sol := [&quot;&quot;, &quot;Khong the giai bai toan&quot;, &quot;&quot;]; return Sol end if; Sol := []; while flag = true do Deduce_RulesVecto() end do; for i in Objects do if NameType(i) &lt;&gt; &quot;?&quot; then FactSetOb := FactSetOb union GetFactsObj(i) end if end do; fSet := FactSet minus Hypos; fSet := [op(fSet), op(FactSetOb), op(FactSet0)]; flag := true; chiso := 0; bt := KiemTraChen(expr, diemChen); if 0 &lt; nops(bt) then Sol := [[[[&quot;&quot;], [&quot;&quot;]], [convert(bt[1], string)], bt[1]], [[[&quot;&quot;], [&quot;&quot;]], [convert(bt[2], string)], bt[2]]]; btgiai := expand(bt[2]); expr := solve(btgiai, goal); expr2 := goal = subs(Facts, expr); Sol := [op(Sol), [[[&quot;&quot;], [&quot;&quot;]], [convert(btgiai, string)], btgiai], [[[&quot;&quot;], [&quot;&quot;]], [convert(expr, string)], expr], [[[&quot;&quot;], [&quot;&quot;]], [convert(expr2, string)], expr2]]; return Sol end if; bt := ApplyRules(expr); if 0 &lt; nops(bt) then Sol := [[[[&quot;&quot;], [&quot;&quot;]], [convert(bt[1], string)], bt[1]], [[[&quot;&quot;], [&quot;&quot;]], [convert(bt[2], string)], bt[2]]]; btgiai := expand(bt[2]); expr := solve(btgiai, goal); expr2 := goal = subs(Facts, expr); Sol := [op(Sol), [[[&quot;&quot;], [&quot;&quot;]], [convert(btgiai, string)], btgiai], [[[&quot;&quot;], [&quot;&quot;]], [convert(expr, string)], expr], [[[&quot;&quot;], [&quot;&quot;]], [convert(expr2, string)], expr2]]; return Sol end if; Sol := [&quot;GoiY_Debai_TuongDuong&quot;, goal, goal]; return Sol end proc:" display=""></Equation></Text-field>
</Input>
