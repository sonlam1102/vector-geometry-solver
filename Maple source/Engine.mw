<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal"><Font foreground="[153,51,255]">SolveVectoLib[Find_VarUnify]:=proc(ex)</Font><Font style="Text">
#tim tap hop cac bien hop nhat co the thay the voi nhau</Font><Font bold="false">
local i,j,flag1,vars,vars1,thaythe,used;
thaythe:={};
vars1:={};</Font><Font style="Text">#vars1 luu cac bien cau truc, vars2 luu cac bien khong co cau truc</Font><Font bold="false" foreground="[255,51,51]">
vars:=Set_Vars(ex);
used:={};
for i in vars do
    if type(i,indexed)or (type(i,function)and op(0,i)=`.`and type(op(2,i),indexed)) then vars1:={op(vars1),i};vars:=vars minus {i};fi;
od;
vars1:=[op(vars),op(vars1)];
for i in vars1 do
   flag1:=false;
   for j in used do
       if Unify_Fact(i,j) then thaythe:={op(thaythe),i=j};flag1:=true;fi;
   od;
   if not flag1 then used:={op(used),i};fi;
od;
return thaythe;</Font><Font foreground="[153,51,255]">
end:

