<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal"><Equation executable="true" style="2D Input" input-equation="SolveVectoLib[SolveVecto_4] := proc (goal) local expr, i, f, temp, temp1, dachen, diemChen, temp2, cf; global flag, FactSet, FactSet0, FactSetOb, Sol, chiso, TestR, fSet, daxetRG; expr := goal; dachen := {}; FactSet := Hypos; FactSet0 := {}; FactSetOb := {}; flag := true; Sol := []; TestR := []; daxetRG := []; expr := expand(lhs(goal)-rhs(goal)); if expr = 0 then Sol := [[&quot;&quot;], [&quot;&quot;], [convert(expr, string)], 0]; return Sol end if; while flag = true do Deduce_RulesVecto() end do; for i in Objects do if NameType(i) &lt;&gt; &quot;?&quot; then FactSetOb := FactSetOb union GetFactsObj(i) end if end do; fSet := FactSet minus Hypos; fSet := [op(fSet), op(FactSetOb), op(FactSet0)]; chiso := 0; flag := true; if has(expr, Doan) = true then Sol := [op(Sol), [[[&quot;&quot;], [&quot;&quot;]], [convert(subs(Doan = Vecto, expr), string)], subs(Doan = Vecto, expr)]]; expr := Sol[nops(Sol)][3] end if; while flag = true do chiso := chiso+1; flag := false; diemChen := DiemChen(expr); if nops(diemChen) = 1 and {op(diemChen)} subset dachen = false then temp := ChenDiemBT(ChuyenVe(expr), diemChen); temp := lhs(temp)-rhs(temp); if Lap_LoiGiai(expr, temp) = false then dachen := {op(dachen), op(diemChen)}; expr := expand(temp); if expr = 0 then Sol := [op(Sol), [[[&quot;&quot;], [&quot;&quot;]], [convert(temp, string), &quot;0&quot;], expr], [[[&quot;&quot;], [&quot;&quot;]], [convert(goal, string)], 0]]; return Sol end if; flag := true; daxetRG := [op(daxetRG), op(convert(temp, list))]; Sol := [op(Sol), [[[&quot;&quot;], [&quot;&quot;]], [convert(temp, string), convert(expr, string)], expr], op(MyFactor0(expr))]; expr := Sol[nops(Sol)][3]; if expr = 0 then return Sol end if end if end if; temp := MyFactor(expr); if 0 &lt; nops(temp) then Sol := [op(Sol), op(temp)]; expr := Sol[nops(Sol)][3]; if expr = 0 then return Sol end if end if; for f in fSet do for i in f[2] do if member([op(f[1]), i], TestR) then next end if; temp := MySubs(i, expr); temp2 := CongBTNhan(expand(temp)); if temp2 = 0 then Sol := [op(Sol), [[[op(f[1])], [i]], [convert(temp, string), temp2], temp2]]; return Sol end if; if Lap_LoiGiai(expr, temp2) = false then if type(op(1, f[1]), symbol) or type(op(1, f[1]), complex) then cf := &quot;&quot; else cf := op(f[1]) end if; flag := true; daxetRG := [op(daxetRG), op(convert(temp, list))]; Sol := [op(Sol), [[[cf], [i]], [convert(temp2, string)], temp2], op(MyFactor0(temp2))]; expr := Sol[nops(Sol)][3]; TestR := {op(TestR), [op(f[1]), i]}; if expr = 0 or Is_Vecto0(expr) = true then return Sol end if end if end do end do end do; temp := Sol[nops(Sol)][3]; Sol := [&quot;GoiY_Debai_TuongDuong&quot;, lhs(goal)-rhs(goal) = 0, temp = 0]; return Sol end proc:" display=""></Equation></Text-field>
</Input>
