<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal"><Font style="Text"># Cac ham tim phan giao,hieu,hoi cua cac tap su kien, xet theo su hop nhat Unify_Fact.</Font><Font foreground="[153,51,255]">
SolveVectoLib[Intersect_Unify] := proc(Aset, Bset)  </Font><Font bold="false">
   local  fact, Cset;
   Cset := {};
   for  fact in Aset  do
      if  Unify_In1(fact, Bset)  then  Cset := {op(Cset), fact};fi;
   od;
   return Cset;</Font><Font foreground="[153,51,255]">
end:  #  Intersect_Unify

SolveVectoLib[Minus_Unify]:= proc( Aset, Bset)</Font><Font bold="false">
 return   Aset minus Intersect_Unify(Aset, Bset);   </Font><Font foreground="[153,51,255]">
