</Font>Sol:=[op(Sol),[[[&quot;&quot;],[&quot;&quot;]],CongVecto(VT - VP)[2],CongVecto(VT - VP)[1]]]<Font bold="false" foreground="[0,0,0]">;
if </Font>CongVecto(VT - VP)[1]<Font bold="false" foreground="[0,0,0]">=Vecto[0] then  flag:=false; return Sol; fi;

if temp &lt;&gt; goal then
   VT := lhs(temp);
   VP :=</Font>Vecto[0]<Font bold="false" foreground="[0,0,0]">;
fi;


</Font><Font bold="false" foreground="[0,255,0]" encoding="UTF-8">#B3 : Ki\341\273\203m tra xem c\303\263 lu\341\272\255t n\303\240o \303\241p d\341\273\245ng \304\221\306\260\341\273\243c v\303\240o bi\341\273\203u th\341\273\251c k?</Font><Font bold="false" foreground="[0,0,0]">
#print(&quot;------Deduce_Rule------&quot;);
while (flag = true ) do
    Deduce_RulesVecto();
od;
for i in Objects do 
     if NameType(i) &lt;&gt; &quot;?&quot; then  
          FactSetOb := `union`(FactSetOb, GetFactsObj(i));
     end if;
end do;
fSet := FactSet minus Hypos;
fSet:= [op(fSet),</Font>op(FactSetOb),<Font bold="false" foreground="[0,0,0]">op(FactSet0)]; 
flag := true;
</Font><Font bold="false">chiso:=0;<Font foreground="[0,0,0]">
while (flag = true) do
    </Font>chiso:=chiso+1;<Font foreground="[0,0,0]">
    flag := false;
    for f in fSet do   
       for i in f[2] do 
             if member([op(f[1]),i],TestR) then next; fi;
